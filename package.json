{
  "name": "movieapp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --non-interactive",
    "android": "expo start --android --non-interactive",
    "ios": "expo start --ios --non-interactive",
    "web": "expo start --web --non-interactive",
    "backend": "cd backend && npm start",
    "backend:dev": "cd backend && npm run dev",
    "dev": "concurrently --kill-others-on-fail --names \"backend,expo\" --prefix-colors \"blue,green\" \"npm run backend:dev\" \"npm start\"",
    "dev:android": "concurrently --kill-others-on-fail --names \"backend,expo\" --prefix-colors \"blue,green\" \"npm run backend:dev\" \"npm run android\"",
    "dev:ios": "concurrently --kill-others-on-fail --names \"backend,expo\" --prefix-colors \"blue,green\" \"npm run backend:dev\" \"npm run ios\""
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "expo": "~54.0.27",
    "expo-linear-gradient": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "private": true
}
